  "connection_types": {
    "mossy_to_glomerulus": {
      "class": "scaffold.connectivity.ConnectomeMossyGlomerulus",
      "presynaptic": {"type": "mossy_fibers", "compartments": ["soma"]},
      "postsynaptic": {"type": "glomerulus", "compartments": ["soma"]}
    },
    "glomerulus_to_granule": {
      "class": "scaffold.connectivity.ConnectomeGlomerulusGranule",
      "presynaptic": {"type": "glomerulus", "compartments": ["soma"]},
      "postsynaptic": {"type": "granule_cell", "compartments": ["dendrites"]},
      "convergence": 4
    },
    "glomerulus_to_golgi": {
      "class": "scaffold.connectivity.ConnectomeGlomerulusGolgi",
      "presynaptic": {"type": "glomerulus", "compartments": ["soma"]},
      "postsynaptic": {"type": "golgi_cell", "compartments": ["dendrites"]}
    },
    "golgi_to_glomerulus": {
      "class": "scaffold.connectivity.ConnectomeGolgiGlomerulus",
      "presynaptic": {"type": "golgi_cell", "compartments": ["axon"]},
      "postsynaptic": {"type": "glomerulus", "compartments": ["soma"]},
      "divergence": 40
    },
    "mossy_to_dcn": {
      "class": "scaffold.connectivity.ConnectomeMossyDCN",
      "presynaptic": {"type": "mossy_fibers", "compartments": ["soma"]},
      "postsynaptic": {"type": "dcn_cell", "compartments": ["dendrites"]},
      "convergence": 50
    },
    "granule_to_golgi": {
      "class": "scaffold.connectivity.ConnectomeGranuleGolgi",
      "presynaptic": {"type": "granule_cell", "compartments": ["parallel_fiber", "ascending_axon"]},
      "postsynaptic": {"type": "golgi_cell", "compartments": ["dendrites"]},
      "tag_aa": "ascending_axon_to_golgi",
      "tag_pf": "parallel_fiber_to_golgi",
      "aa_convergence": 400,
      "pf_convergence": 1200
    },
    "golgi_to_granule": {
      "class": "scaffold.connectivity.ConnectomeGolgiGranule",
      "presynaptic": {"type": "golgi_cell", "compartments": ["axon"]},
      "postsynaptic": {"type": "granule_cell", "compartments": ["dendrites"]},
      "after": ["golgi_to_glomerulus", "glomerulus_to_granule"]
    },
    "ascending_axon_to_purkinje": {
      "class": "scaffold.connectivity.ConnectomeAscAxonPurkinje",
      "presynaptic": {"type": "granule_cell", "compartments": ["ascending_axon"]},
      "postsynaptic": {"type": "purkinje_cell", "compartments": ["dendrites"]}
    },
    "parallel_fiber_to_purkinje": {
      "class": "scaffold.connectivity.ConnectomePFPurkinje",
      "presynaptic": {"type": "granule_cell", "compartments": ["parallel_fiber"]},
      "postsynaptic": {"type": "purkinje_cell", "compartments": ["dendrites"]}
    },
    "parallel_fiber_to_basket": {
      "class": "scaffold.connectivity.ConnectomePFInterneuron",
      "presynaptic": {"type": "granule_cell", "compartments": ["parallel_fiber"]},
      "postsynaptic": {"type": "basket_cell", "compartments": ["dendrites"]},
      "after": ["granule_to_golgi"]
    },
    "parallel_fiber_to_stellate": {
      "class": "scaffold.connectivity.ConnectomePFInterneuron",
      "presynaptic": {"type": "granule_cell", "compartments": ["parallel_fiber"]},
      "postsynaptic": {"type": "stellate_cell", "compartments": ["dendrites"]},
      "after": ["granule_to_golgi"]
    },
    "basket_to_purkinje": {
      "class": "scaffold.connectivity.ConnectomeBCSCPurkinje",
      "presynaptic": {"type": "basket_cell", "compartments": ["axon"]},
      "postsynaptic": {"type": "purkinje_cell", "compartments": ["dendrites"]},
      "divergence": 2,
      "convergence": 20,
      "limit_x": 500.0,
      "limit_z": 100.0
    },
    "stellate_to_purkinje": {
      "class": "scaffold.connectivity.ConnectomeBCSCPurkinje",
      "presynaptic": {"type": "stellate_cell", "compartments": ["axon"]},
      "postsynaptic": {"type": "purkinje_cell", "compartments": ["dendrites"]},
      "divergence": 2,
      "convergence": 20,
      "limit_x": 500.0,
      "limit_z": 100.0
    },
    "stellate_to_stellate": {
      "class": "scaffold.connectivity.ConnectomeGapJunctions",
      "presynaptic": {"type": "stellate_cell", "compartments": ["axon"]},
      "postsynaptic": {"type": "stellate_cell", "compartments": ["dendrites"]},
      "divergence": 4,
      "limit_xy": 150.0,
      "limit_z": 50.0
    },
    "basket_to_basket": {
      "class": "scaffold.connectivity.ConnectomeGapJunctions",
      "presynaptic": {"type": "basket_cell", "compartments": ["axon"]},
      "postsynaptic": {"type": "basket_cell", "compartments": ["dendrites"]},
      "divergence": 4,
      "limit_xy": 150.0,
      "limit_z": 50.0
    },
    "golgi_to_golgi": {
      "class": "scaffold.connectivity.ConnectomeGapJunctionsGolgi",
      "presynaptic": {"type": "golgi_cell", "compartments": ["axon"]},
      "postsynaptic": {"type": "golgi_cell", "compartments": ["dendrites"]}
    },
    "purkinje_to_dcn": {
      "class": "scaffold.connectivity.ConnectomePurkinjeDCN",
      "presynaptic": {"type": "purkinje_cell", "compartments": ["axon"], "with_label": "microzone-*"},
      "postsynaptic": {"type": "dcn_cell", "compartments": ["dendrites"], "with_label": "microzone-*"},
      "divergence": 5
    },
    "purkinje_to_dcn_interneuron": {
      "class": "scaffold.connectivity.SatelliteCommonPresynaptic",
      "presynaptic": {"type": "purkinje_cell", "compartments": ["axon"], "with_label": "microzone-*"},
      "postsynaptic": {"type": "dcn_interneuron", "compartments": ["dendrites"], "with_label": "microzone-*"},
      "after": ["purkinje_to_dcn"]
    },
    "io_to_purkinje": {
      "class": "scaffold.connectivity.ConnectomeIOPurkinje",
      "presynaptic": {"type": "io_cell", "compartments": ["axon"], "with_label": "microzone-*"},
      "postsynaptic": {"type": "purkinje_cell", "compartments": ["dendrites"], "with_label": "microzone-*"},
      "divergence": 6
    },
    "io_to_basket": {
      "class": "scaffold.connectivity.ConnectomeIOMolecular",
      "presynaptic": {"type": "io_cell", "compartments": ["axon"]},
      "postsynaptic": {"type": "basket_cell", "compartments": ["dendrites"]},
      "after": ["io_to_purkinje", "basket_and_stellate_to_purkinje"]
    },
    "io_to_stellate": {
      "class": "scaffold.connectivity.ConnectomeIOMolecular",
      "presynaptic": {"type": "io_cell", "compartments": ["axon"]},
      "postsynaptic": {"type": "stellate_cell", "compartments": ["dendrites"]},
      "after": ["io_to_purkinje", "basket_and_stellate_to_purkinje"]
    },
    "io_to_dcn": {
      "class": "scaffold.connectivity.AllToAll",
      "presynaptic": {"type": "io_cell", "compartments": ["axon"], "with_label": "microzone-*"},
      "postsynaptic": {"type": "dcn_cell", "compartments": ["dendrites"], "with_label": "microzone-*"}
    },
    "io_to_dcn_interneuron": {
      "class": "scaffold.connectivity.AllToAll",
      "presynaptic": {"type": "io_cell", "compartments": ["axon"], "with_label": "microzone-*"},
      "postsynaptic": {"type": "dcn_interneuron", "compartments": ["dendrites"], "with_label": "microzone-*"}
    },
    "dcn_interneuron_to_io": {
      "class": "scaffold.connectivity.AllToAll",
      "presynaptic": {"type": "dcn_interneuron", "compartments": ["axon"], "with_label": "microzone-*"},
      "postsynaptic": {"type": "io_cell", "compartments": ["dendrites"], "with_label": "microzone-*"}
    }
  },
