{
  "name": "Test multiple cell types",
  "storage": {
    "engine": "hdf5"
  },
  "network": {
    "x": 100.0,
    "y": 50.0,
    "z": 100.0,
    "chunk_size": [
      50,
      50,
      50
    ]
  },
  "partitions": {
    "test_layer": {
      "thickness": 50
    }
  },
  "cell_types": {
    "from_cell": {
      "spatial": {
        "radius": 2.5,
        "count": 4
      }
    },
    "to_cell": {
      "spatial": {
        "radius": 2.5,
        "count": 4
      }
    }
  },
  "placement": {
    "placement": {
      "strategy": "bsb.placement.ParticlePlacement",
      "cell_types": [
        "from_cell", "to_cell"
      ],
      "partitions": [
        "test_layer"
      ]
    }
  },
  "connectivity": {
    "connection": {
      "strategy": "bsb.connectivity.AllToAll",
      "presynaptic": {
        "cell_types": [
          "from_cell"
        ]
      },
      "postsynaptic": {
        "cell_types": [
          "to_cell"
        ]
      }
    }
  },
  "simulations": {
    "test_double_neuron": {
      "simulator": "nest",
      "duration": 10,
      "cell_models": {
        "from_cell": {
          "constants": {
            "t_ref": 1.5,
            "C_m": 7.0,
            "V_reset": -70.0,
            "E_L": -62.0,
            "I_e": 0.0,
            "V_th": -41.0
          }
        },
        "to_cell": {
          "constants": {
            "t_ref": 1.5,
            "C_m": 7.0,
            "V_reset": -70.0,
            "E_L": -62.0,
            "I_e": 0.0,
            "V_th": -41.0
          }
        }
      },
      "connection_models": {
      },
      "devices": {
      }
    }
  }
}
